<%- include("./partials/header.ejs") %>

<body>
  <div class="ui container fluid custom-home-bg" id="home-container">
    <%- include("./partials/navbar.ejs") %> 
    <div class="ui left demo vertical inverted sidebar labeled icon menu">
      <a class="item">
        <i class="icon gamepad"></i>
        Gaming
      </a>
      <a class="item">
        <i class="tv icon"></i>
        Streaming
      </a>

    </div>
    <div class="ui equal width center alligned padded grid">
      <div class="row">
        <div class="four wide column">

        </div>
        <div class="eight wide column">



          <table class="ui celled striped table">
            <thead>
              <tr>
                <th colspan="3">
                  Message List
                </th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>
                  <a href="/viewmsguser">
                    <button class="ui primary button">
                      Create New Message
                    </button>
                  </a>
                </td>
              </tr>
              <% if (data.msgList.length == 0) { %>
                <tr>
                  <td>
                    No Messages Found

                  </td>
                </tr>
              <% }else{ %>
                <% data.msgList.forEach(msg => { %>
              <tr>
                <td class="collapsing">

                  <a href="/message-inbox?uid=<%= msg.created_with_id %> "><i class="comments icon"></i> <%= msg.user_name %></a>
                </td>
               
              </tr>
              <%}); } %>
              
            </tbody>
          </table>



        </div>
        <div class="four wide column">

        </div>
      </div>
    </div>

  </div>







</body>

</html>
<%- include("./partials/footer.ejs") %> 